<div class="container fluid mt-5">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto about-style" id="contactid">
                <h1 class="text-center main-heading">Contact Us</h1>
                
                <div class="row">


<form action="/contact" method="POST">
<div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" id="c" name="name" placeholder="name">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"  name="email"placeholder="Enter email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  
  <div class="form-group">
    <label for="e">Phone</label>
    <input type="number" class="form-control" id="e" name="phone" placeholder="Phone">
  </div>
  <div class="form-group">
    <label for="d">Message</label>
    <input type="text" class="form-control" id="d" name="message" placeholder="Message">
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</div>
</div>
</div>